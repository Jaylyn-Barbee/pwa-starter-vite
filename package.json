{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "start": "npm run build && vite preview",
    "start-remote": "vite --host"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fluentui/web-components": "^2.2.2",
    "@pwabuilder/pwainstall": "^1.6.7",
    "@vaadin/router": "^1.7.4",
    "browser-fs-access": "^0.25.0",
    "lit": "^2.2.0",
    "workbox-build": "^6.4.2",
    "workbox-core": "^6.4.2",
    "workbox-precaching": "^6.4.2"
  },
  "devDependencies": {
    "typescript": "4.3.5",
    "vite": "^2.8.0",
    "vite-plugin-pwa": "^0.11.13"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
